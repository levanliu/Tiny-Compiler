extern int printf(string str);
extern int puts(string str);
int minus(int a, int b)
{
    int c;
    c = a - b;
    return c;
}
int add(int a, int b)
{
    int c;
    c = a + b;
    return c;
}
int do (int a, int b)
{
    int c;
    c = add(a, b) + minus(a, b);
    return c;
}

int main()
{
    int a = 100;
    int b = 200;

    int c;
    c = do(add(a, b), minus(b, a));

    printf("%d",c);
    puts("");

    return 0;
}
