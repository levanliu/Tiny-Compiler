extern int printf(string str);
extern int puts(string str);
int main(){

    int[13] arr = [ 0,1,2,3,5,7,8,1,3,2,4,6 ];
    int[3][4] arr2;

    int i;
    int j;

    for(i=0; i<3; i=i+1){
        for(j=0; j<4; j=j+1){
            arr2[i][j] = arr[i*4+j];
        }
    }

    for(i=0; i<3; i=i+1){
        for(j=0; j<4; j=j+1){
            printf("%d,", arr2[i][j]);
        }
        puts("");
    }


    return 0;
}
